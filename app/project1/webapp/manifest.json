{
  "_version": "1.65.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.1",
      "toolsId": "bd03cefb-cc95-460e-b684-21df41d58c63"
    },
    "dataSources": {
      "mainService": {
        "uri": "/service/genAI/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.129.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "synchronizationMode": "None",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "suggestedQuestions": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/suggestedQuestions.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "project1.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "no",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "standard",
          "target": [
            "standard"
          ]
        },
        {
          "pattern": "expert",
          "name": "expert",
          "target": [
            "expert"
          ]
        },
        {
          "pattern": "standard",
          "name": "standard",
          "target": [
            "standard"
          ]
        }
      ],
      "targets": {
        "standard": {
          "viewId": "standard",
          "viewName": "Standard"
        },
        "expert": {
          "viewId": "expert",
          "viewName": "Expert"
        }
      }
    },
    "rootView": {
      "viewName": "project1.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "lcap.product_search_rag"
  }
}