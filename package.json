{
  "name": "product_search_rag_YOUR_NUMBER",
  "version": "1.0.0",
  "description": "Product Search RAG App",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^3.26.1",
    "@sap-cloud-sdk/http-client": "^3.16.0",
    "@sap-cloud-sdk/util": "^3.16.0",
    "@sap/cds": "^7",
    "@sap/cds-hana": "^2",
    "@sap/low-code-event-handler": "^1.2.4",
    "@sap/mdk-tools": "^1.3.1",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^3.6.1",
    "cap-llm-plugin": "^1.4.4",
    "express": "^4",
    "hdb": "^0.19.8",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/dev-cap-tools": "^1.31.0",
    "@sap/eslint-plugin-cds": "^3",
    "@sap/ux-specification": "^1.120.13",
    "eslint": "^9",
    "rimraf": "^3.0.2",
    "cds-plugin-ui5": "^0.9.3"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-scale_assets_search": "cds watch --open scale_assets_search/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy product_search_rag_YOUR_NUMBER --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-productsearchrag": "cds watch --open productsearchrag/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "requires": {
      "gen-ai-hub": {
        "gpt-4o": {
          "destinationName": "GenAIHubDestination",
          "deploymentUrl": "/v2/inference/deployments/YOUR_GPT4O_MODEL_ID",
          "resourceGroup": "userYOUR_NUMBER-l2o-rg",
          "apiVersion": "2024-02-15-preview",
          "modelName": "gpt-4o"
        },
        "text-embedding-ada-002": {
          "destinationName": "GenAIHubDestination",
          "deploymentUrl": "/v2/inference/deployments/YOUR_EMBEDDING_MODEL_ID",
          "resourceGroup": "userYOUR_NUMBER-l2o-rg",
          "apiVersion": "2024-02-15-preview",
          "modelName": "text-embedding-ada-002"
        }
      },
      "GenAIHubDestination": {
        "kind": "rest",
        "credentials": {
          "destination": "aicore-destination",
          "requestTimeout": "300000"
        }
      },
      "PythonSrvDestination": {
        "kind": "rest",
        "credentials": {
          "destination": "python-srv",
          "requestTimeout": "300000"
        }
      },
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "cap-llm-plugin": {
        "impl": "cap-llm-plugin/srv/cap-llm-plugin.js"
      },
      "app-service": {
        "impl": "@sap/low-code-event-handler"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      }
    },
    "features": {
      "fetch_csrf": true
    }
  },
  "lcap": true,
  "workspaces": [
    "app/*"
  ]
}
